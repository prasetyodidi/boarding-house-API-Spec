{
  "openapi": "3.0.3",
  "info": {
    "title": "Boarding House  OpenAPI",
    "description": "API Specification for Boarding House",
    "version": "1.0.0",
    "contact": {
      "name": "Didi Prasetyo",
      "email": "didiprasetyo360@gmail.com",
      "url": "https://prasetyodidi.github.io/"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "servers": [
    {
      "url": "http://localhost:8080"
    }
  ],
  "paths": {
    "/rules/{boardingHouseId}": {
      "get": {
        "parameters": [
          {
            "$ref": "#/components/parameters/boardingHouseId"
          }
        ],
        "description": "get all rules",
        "responses": {
          "200": {
            "$ref": "#/components/responses/rulesSuccessResponse"
          }
        }
      }
    },
    "/reminder/{boardingHouseId}": {
      "get": {
        "parameters": [
          {
            "$ref": "#/components/parameters/boardingHouseId"
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/components/responses/reminderSuccessResponse"
          }
        }
      }
    },
    "/boarding-house": {
      "get": {
        "parameters": [
          {
            "$ref": "#/components/parameters/ownerIdQuery"
          },
          {
            "$ref": "#/components/parameters/tenantIdQuery"
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/components/responses/boardingHoseSuccessResponse"
          }
        }
      }
    }
  },
  "components": {
    "parameters": {
      "boardingHouseId": {
        "name": "boardingHouseId",
        "description": "boarding house id",
        "in": "path"
      },
      "ownerIdQuery": {
        "in": "query",
        "description": "selection boarding house by owner id",
        "name": "ownerId",
        "schema": {
          "minLength": 1,
          "type": "string",
          "required": false
        }
      },
      "tenantIdQuery": {
        "in": "query",
        "description": "selection boarding house by tenant id",
        "name": "tenantId",
        "schema": {
          "minLength": 1,
          "type": "string",
          "required": false
        }
      }
    },
    "responses": {
      "rulesSuccessResponse": {
        "description": "response get rules",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "required": true
                },
                "message": {
                  "type": "string",
                  "required": true
                },
                "data": {
                  "type": "array",
                  "required": true,
                  "items": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      },
      "reminderSuccessResponse": {
        "description": "response get reminder",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "required": true
                },
                "message": {
                  "type": "string",
                  "required": true
                },
                "data": {
                  "$ref": "#/components/schemas/reminder"
                }
              }
            }
          }
        }
      },
      "boardingHoseSuccessResponse": {

        "description": "success get list boarding house",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string"
                },
                "message": {
                  "type": "string"
                },
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "address": {
                        "type": "string"
                      },
                      "image_url": {
                        "type": "string"
                      }
                    }
                  }
                }
              },
              "required": ["status", "message", "data"]
            }
          }
        }
      }
    },
    "schemas": {
      "room": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32",
            "required": true
          },
          "name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255,
            "required": true
          },
          "owner": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255,
            "required": false
          },
          "image_url": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255,
            "required": false
          }
        }
      },
      "boardingHouse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32",
            "required": true
          },
          "address": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255,
            "required": true
          },
          "owner": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255,
            "required": false
          },
          "image_url": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255,
            "required": false
          }
        }
      },
      "reminder": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "reminder": {
              "type": "string"
            },
            "image_url": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
